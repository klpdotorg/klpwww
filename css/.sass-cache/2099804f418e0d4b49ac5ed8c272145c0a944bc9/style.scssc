			li.place_item{
				display:block; background: #f8f8f8; padding: 20px; zoom:1;
				border-top: 1px solid #fff; border-bottom: 1px solid #d8d8d8;

				&:before, &:after{
					content:"";
					display: table;
				}
				&:after{
					clear: both;
				}

				&:hover{
					background: #fff; cursor: pointer;
				}

				.details{
					float: left;
					h3{
						color: $color_maps_school; font-size: 16px; line-height: 26px; margin: 0;
						text-align: left; text-transform: none; 
						font-weight: normal; letter-spacing: normal;
					}
					.info{
						color: #898989;  font-size: 12px; line-height: 18px;
					}
				}
				.stats{
					float: right; text-align: center;

					.distance{
						text-transform: uppercase; font-weight: bold;
					}
				}
				
				
			}
			li.active{
				background: #fff;
			}
		}
	}

	.map_overlay{
		position: absolute; z-index: 9; right:20px; left: 340px; margin-bottom: 20px;

		.close_btn{
			position: absolute; z-index:10; top:15px; right: 25px; top:-10px; right: -10px; 
			text-transform: uppercase; color: #393939; font-size: 12px; line-height: 35px; font-weight: bold;
			span{
				display: block; float: right; width:35px; height:35px;
				background: #f8f8f8 url("../images/close_icon.png") no-repeat center center;
				border-radius: 50%;
			}
		}
		
		.content{
			margin: 0 0 25px;
			background: rgba(255,255,255,0.8);
			-moz-box-shadow: 0 0px 4px 0 rgba(77,77,77,0.30);
			-webkit-box-shadow: 0 0px 4px 0 rgba(77,77,77,0.30);
			box-shadow: 0 0px 4px 0 rgba(77,77,77,0.30);
		}

		.effect_to_be_done{
			height: 0px;
		}
		.info{
			padding: 20px 25px 25px;
			h2{
				color: $color_maps_school; font-size: 20px; line-height: 32px; margin: 0;
				text-align: left; font-weight: normal; text-transform: none;
				text-shadow: 1px 1px 0px rgba(255,255,255,0.5);
			}
			.item{
				float: left; margin-right: 50px;
				.label,.value{
					text-transform: uppercase; font-size: 11px; line-height: 18px; 
					color: #898989; font-weight: bold; letter-spacing: 1px;
				}
				.value{
					text-transform: none; color: #393939; letter-spacing: normal; font-size: 12px;
				}
			}
		}
		ul.tabs{
			list-style: none; margin-left: 25px; zoom:1;
			&:before, &:after{
				content:"";
				display: table;
			}
			&:after{
				clear: both;
			}
			li{
				float:left; margin-right: 5px;
				&:last-child{ margin-right:0; }
				
					display: block; font-size: 10px; line-height: 32px; font-weight: bold;
					text-transform: uppercase; color: #393939; letter-spacing: 1px;
					padding: 0 10px; text-decoration: none;
				&:hover{
					cursor: pointer;
				}
			}
			li.current,li:hover{
				background: #fff; border: 1px solid #d8d8d8; border-bottom: none;
				line-height: 31px; padding: 0 9px;
			}
		}
		.tab_content{
			background: #fff; padding: 0 25px 25px; border-top: 1px solid #d8d8d8; margin-top: -1px;
			display: none;

			h4{
				font-size: 12px; line-height: 12px; font-weight: bold; color: #7B7795;
				text-align: left; margin:25px 0 10px; letter-spacing: 1px;
			}

			p{
				font-size: 13px; line-height: 20px; margin: 0;
			}

			a.black{
				color: #4D4D4D; font-size: 13px;line-height: 24px;
			}

			.photos_block{
				float: right; width:280px;
				img{
					width:60px; height: 60px; border: 0px solid #d8d8d8;
					margin: 0 10px 10px 0; float: left;
				}
			}

			.top_bordered_block{
				border-top:1px solid #EDEDED; margin-top: 15px; padding-top: 15px;
				p{
					margin: 5px 0;
				}
				b,a{
					color: #4D4D4D;
				}
			}

			.pie_chart_wrapper{
				.chart{
					width:170px; height:170px; background:#f8f8f8; margin-right: 25px;
				}
				.gender_icons{
					margin-top:25px;
				}
			}

			.options_wrapper{
				text-align: center; margin-top: 25px;
				.select_pseudo{
					margin-right: 25px;
				}
			}

			.option_item{
				display: inline; background-color: #F9F9F9; font-size: 13px; line-height: 13px;
				border:1px solid #d8d8d8; padding:5px 10px;
				background-position: right center; background-repeat: no-repeat;
				ul{
					displays: none; list-style: none; position: absolute; width: 100%;
					background: #fff;
				}
			}
			.option_item.dropdown{
				background-image: url("../images/status_dropdown_dark.png");
			}
		}
		.tab_content.current{
			display: block;
		}
	}
}

.info_data_table{
	border: none; border-collapse: collapse;
	tr{
		td{
			font-size: 13px; line-height: 24px; text-align: left;
			&:first-child{
				font-weight:bold; width:150px; color: #393939
			}
		}
	}
}
.info_data_table.label_small{
	tr td:first-child{
		width:100px;
	}
}
.info_data_table.label_long{
	tr td:first-child{
		width:200px;
	}
}
.info_data_table.label_longer{
	tr td:first-child{
		width:300px;
	}
}

/* Infrastructure */
.infra_data_table,.infra_data_indicators{
	border: none; border-collapse: collapse; min-width: 500px; margin-right: 10px;
	tr{
		td{
			font-size: 13px; line-height: 24px; text-align: left; vertical-align: top;
			border-bottom: 1px solid #d8d8d8; padding: 10px 0;
			&:first-child{
				font-weight:bold; width:180px; color: #393939
			}
		}
		&:last-child{
			td{
				border-bottom: none;
			}
		}
	}
	.green{
		color: #1DA84B; padding-right: 25px;
		background: url("../images/icon_tick.png") no-repeat right center;
	}
	.red{
		color: #BF0002; padding-right: 25px;
		background: url("../images/icon_cross.png") no-repeat right center;
	}
	.grey{
		color: #6D6D6D; padding-right: 25px;
		background: url("../images/icon_null.png") no-repeat right center;
	}
}
.infra_data_indicators{
	border: 1px solid #d8d8d8; padding:10px; min-width: inherit;
	-moz-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px;
	.item{
		background-position: left center; padding: 0 0 0 25px;
		font-size: 13px; line-height: 24px;
	}
}

.bar_graph_hor{
	width: 350px;
	.item{
		margin-bottom: 20px; zoom:1;
		&:before, &:after{
			content:"";
			display: table;
		}
		&:after{
			clear: both;
		}
		.label{
			color: #393939; font-size: 13px; text-transform: uppercase; font-weight: bold;
			margin-right: 10px; float: left;
		}
		.bar{
			width: 250px; height: 15px; background: #EDEDED; float: right;
			.value{
				background: #DA5765; height: inherit; width: 0;
			}
		}
	}
}

.library_table{
	width: 80%; border: none; margin: 0 auto; border-collapse: collapse;
	border: 1px solid #d8d8d8;
	tr{
		
		th,td{
			width:14.285%; 
			text-align: center; padding: 10px 0; font-size: 13px;
		}
		th{
			background: #f6f6f6; line-height: 20px;
		}
		td{
			&:first-child{
				text-transform:uppercase;
			}
		}
	}
}

.select_pseudo{
	display:inline-block; position: relative; 
	margin-bottom: 0px; text-align: left; border:1px solid #D8D8D8;
	
	select{
		display: none;
	}

	.button_wrapper{
		padding: 0 7px 0 10px; background: #f6f6f6; 
		&:hover{
			cursor: pointer;
		}
	}

	
	button{
		border: none; padding:9px 14px 8px 0; margin: 0; color: #6d6d6d;
		background: url("../images/status_dropdown_dark.png") no-repeat right center;
		font-size: 13px; line-height: 13px;
		border: none; outline: none;

		&:hover{
			cursor: pointer;
		}
	}

	ul{
		display:none; list-style: none; position: absolute;  width: 100%; 
		background: #fff; margin-left: -1px; 
		border:1px solid #D8D8D8; border-top: none;
		li{
			width: auto; padding:10px 15px; font-size: 14px; line-height: 14px;
			color: #6d6d6d;
			&:hover{
				background:#f6f6f6;
			}
		}
	}

	&:hover{
		ul{
			display: block;
		}
	}
}

button.styled{
	border:1px solid #D8D8D8; padding: 9px 10px 8px 10px; background: #f6f6f6; outline: none;
	font-size: 13px; line-height: 13px; color: #6d6d6d;
	&:hover{
		cursor: pointer;
	}
}






