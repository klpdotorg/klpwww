/*** Main Header ***/

.main-header{

	.header-wrapper{
		width:100%; background:#fff;
		border-bottom: 1px solid #d8d8d8;

		.header-content{
			width:960px; margin: 0 auto;
		}

		h1{
			background: url("../images/KLP_logo.png") 0 center no-repeat;
			text-indent: 100%;
			white-space: nowrap;
			overflow: hidden;
			width:100px; float: left;
			padding: 0; margin: 0; line-height: 50px;
		}
	}

	.top-nav{
		float: right; width:860px;

		ul{
			&:before, &:after{
				content:"";
				display: table;
			}
			&:after{
				clear: both;
			}

			list-style: none; float: right; zoom:1;
			width:100%; margin: 0; padding: 0;

			li{
				display:block; width:143.33px; float: left;
				margin: 0; padding: 0;

				a{
					display:block; text-align: center; line-height: 45px;
					margin: 0 3px; background: #fff;
					border-top: 5px solid #ccc;
					font-size: 11px; letter-spacing: 1px; font-weight: 700;
					text-transform: uppercase;

					&:hover{
						text-decoration: none;
					}
				}

				> &:first-child a{  margin-left: 0; }

				> &:last-child a{  margin-right: 0;  }

				&:hover > ul{  display:block;  }
			}

			li.home a{
				background: url("../images/KLP_logo.png") center center no-repeat;
				text-indent: 100%; white-space: nowrap; overflow: hidden;
				border-top: 2px solid #fff; 
			}

			@mixin top-nav_ul_li($color) {
				border-top: 5px solid $color; color:$color;
			}

			li.school_map a {
				@include top-nav_ul_li($color_school_map);
			}
			li.programmes a {
				@include top-nav_ul_li($color_programmes);
			}
			li.volunteer a {
				@include top-nav_ul_li($color_volunteer);
			}
			li.database a {
				@include top-nav_ul_li($color_database);
			}
			li.reports a {
				@include top-nav_ul_li($color_reports);
			}
			li.about a {
				@include top-nav_ul_li($color_about);
			}

			ul{
				display:none; float: none; background:#fff;
				position: absolute; z-index:100; top:50px; width:140px;
				padding: 10px 0; margin:0;
				-moz-box-shadow: 0 2px 2px 0 rgba(77,77,77,0.30);
				-webkit-box-shadow: 0 2px 2px 0 rgba(77,77,77,0.30);
				box-shadow: 0 2px 2px 0 rgba(77,77,77,0.30);

				li{
					display:block; width:100%; float: none;
					margin: 0; padding: 0;

					a{
						display:block; padding: 10px 20px;
						text-align: left; background: #fff;
						font-size: 12px; line-height:12px;
						letter-spacing: normal; font-weight: 400;
						text-transform: none; border-top: none!important;

						span{
							display:inline-block; height: 10px; padding:0 5px;
						}
					}

					&:hover a span{
						border-left:2px solid #6d6d6d; padding:0 4px;
					}
				}
			}

			@mixin top-nav_ul_li_ul_li_hover($color) {
				border-left: 2px solid $color;
			}

			li.school_map ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_school_map);
			}
			li.programmes ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_programmes);
			}
			li.volunteer ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_volunteer);
			}
			li.database ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_database);
			}
			li.reports ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_database);
			}
			li.about ul li:hover a span {
				@include top-nav_ul_li_ul_li_hover($color_about);
			}
		}
	}
}

/** Sub header **/

.main-header .subheader-wrapper{
	width:100%; background:#f6f6f6;
	border-bottom: 0px solid #d8d8d8;
	-moz-box-shadow: 0 2px 4px 0 rgba(77,77,77,0.30);
	-webkit-box-shadow: 0 2px 4px 0 rgba(77,77,77,0.30);
	box-shadow: 0 2px 4px 0 rgba(77,77,77,0.30);

	.subheader-content{
		width:960px; margin: 0 auto;
	}

	/** Breadcrumb ****/
	.breadcrumb{
		font-size: 10px; line-height: 32px;  text-transform: uppercase;
		letter-spacing: 1.4px; font-weight: normal;
		color: #6d6d6d;

		a{	color: #6d6d6d;	}

		span{
			padding: 0 10px; background: url("../images/breadcrumb_icon2.png") no-repeat center center;
		}
	}
}

.main-header.fixed{
	position: fixed; z-index: 10; width:100%;
	.header-wrapper{
		.header-content{
			width:100%;
		}

		h1{
			display: none;
		}
	}
	.top-nav{
		width:100%;
	}
	.top-nav > ul > li{
		width:14.28%;
		ul{
			width: inherit;
		}
	}
	.subheader-wrapper{
		.subheader-content{
			width:auto; padding: 0 25px;
		}
	}
}


